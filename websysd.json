{
	"Environment": {
		"WEBSYSD": "true"
	},
	"Tasks": [
		{
			"Name": "sleep",
			"Command": "sleep 60",
			"Environment": {},
			"Service": true
		},
		{
			"Name": "echo",
			"Command": "echo \"This is a test: $PORT\"",
			"Executor": ["/bin/sh", "-c"],
			"Environment": {
				"PORT": "9911"
			},
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		},
		{
			"Name": "env",
			"Command": "env",
			"Executor": ["/bin/sh", "-c"],
			"Stdout": "/tmp/$TASK-$RUN.out",
			"Stderr": "/tmp/$TASK-$RUN.err"
		}
	]
}
